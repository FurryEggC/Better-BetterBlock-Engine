xPrev = x;
yPrev = y;

h = 0;
if place_meeting(x-1, y, objPlayer) &amp;&amp; scrButtonCheck(global.rightButton) {
    var flag = 1;
    with (objPlayer) {
        if (place_meeting(x + 1, y, objBetterBlock)) {
            flag = 0;
        }
    }
    if (flag) h = pushSpeed;
}
if place_meeting(x+1, y, objPlayer) &amp;&amp; scrButtonCheck(global.leftButton) {
    var flag = 1;
    with (objPlayer) {
        if (place_meeting(x - 1, y, objBetterBlock)) {
            flag = 0;
        }
    }
    if (flag) h = -pushSpeed;
}
if (!airPush &amp;&amp; !place_meeting(x, y+1*sign(grav), objBetterBlockBase)) {
    h = 0;
}

v += grav;
if (abs(v) &gt; maxFallSpeed) {
    v = sign(v) * maxFallSpeed;
}

// Move horizontally, pixel by pixel, until hit a wall or fall off a ledge.
scrMutiPushBlockMove();


// Move vertically either all at once, or pixel by pixel if hitting the floor.

if not place_meeting(x, y+v, objBetterBlockBase) {
    y += v;
}
else {
    repeat (abs(v) * 10) {
        y += sign(v) * grav;
        if place_meeting(x, y, objBetterBlockBase) {
            y -= sign(v) * grav;
            break;
        }
    }
    v = -grav;
}

/*if place_meeting(x, y, objPlayer) {
